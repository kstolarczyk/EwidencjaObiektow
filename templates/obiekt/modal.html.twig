{% trans_default_domain 'App' %}
{% extends 'modal.base.html.twig' %}
{% block modal_id %}obiekt-modal{% endblock %}
{% block modal_zindex %}1060{% endblock %}
{% block modal_title_id %}obiekt-modal-title{% endblock %}
{% block modal_size %}mw-100 w-auto p-0 m-1 m-lg-2 modal-dialog-centered{% endblock %}
{% block modal_body %}
    <div id="obiekt-modal-content"></div>
{% endblock %}
{% block modal_footer %}
    <button class="btn btn-primary btn-lg" type="button" id="obiekt-modal-zapisz" onclick="Zapisz(this)">{{ 'Zapisz'|trans }}</button>
    {{ parent() }}
{% endblock %}