{% trans_default_domain 'App' %}
{{ form_start(form) }}
{{ form_row(form.nazwa) }}
{{ form_row(form.symbol) }}
{{ form_row(form.grupa) }}
{{ form_end(form) }}
<script>
    $("#obiekt_grupa").change(function () {
        let form = $("#obiekt-modal-content").find("form");
        $.post("{{ path('obiekt_dodaj') }}", form.serialize(), function (response, status) {
            if (status === "success") {
                $("#obiekt-modal-content").html(response);
            }
        });
    });
</script>